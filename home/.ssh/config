host iphone
	HostName localhost
	User root
	Port 2222

host iphone-other
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	HostName localhost
	User root
	Port 2222

host iphone-wifi
	HostName 192.168.1.102
	User root

host ec2
	HostName ec2-54-228-28-167.eu-west-1.compute.amazonaws.com
	User ec2-user
	IdentityFile ~/.ssh/ec2.pem

host irc
	HostName irc.tyilo.com
	User tyilo

host fh
	HostName fh.cs.au.dk
	User tyilo

host udoo
	HostName 192.168.1.105
	User ubuntu
